{
    "name": "michaeljberry/laravel-packager",
    "description": "A cli tool for creating Laravel packages.",
    "keywords": [
        "Packager",
        "Laravel",
        "package",
        "skeleton",
        "Github"
    ],
    "homepage": "https://github.com/michaeljberry/laravel-packager",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "michaeljberry",
            "email": "michael@michaeljberry.com",
            "homepage": "https://github.com/michaeljberry"
        }
    ],
    "require": {
        "illuminate/support": "~5",
        "illuminate/console": "~5",
        "illuminate/filesystem": "~5",
        "guzzlehttp/guzzle": "~6",
        "sensiolabs/security-checker": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "michaeljberry\\Packager\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "michaeljberry\\Packager\\PackagerServiceProvider"
            ]
        }
    }
